<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fffcef"
    android:orientation="vertical">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#fff2c1"        >
        <ImageButton
            android:id="@+id/back"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="#00000000"
            android:layout_gravity="center|left"
            android:src="@drawable/back2"
            android:layout_marginLeft="10dp"/>
        <ImageView
            android:layout_gravity="top|right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/sun2"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="자주 묻는 질문"
            android:textColor="@color/black"
            android:layout_margin="5dp"
            android:layout_gravity="center"
            android:fontFamily="@font/permanentmarkerregular"
            android:textSize="30dp"/>
    </FrameLayout>
    <LinearLayout
        android:layout_marginTop="10dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="370dp"
        android:layout_height="1.2dp"
        android:background="@drawable/line"/>
    <TextView
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FAQ"
        android:textSize="20dp"
        android:textColor="#1C1C1C"/>
    <LinearLayout
        android:layout_marginTop="10dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="370dp"
        android:layout_height="1.2dp"
        android:background="@drawable/line2"/>
    <LinearLayout
        android:layout_marginTop="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:layout_marginLeft="20dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="우리는 한이음에서 수상할 수 있을까요?"
            android:textSize="15dp"
            android:textColor="#1C1C1C"/>
        <ImageView
            android:id="@+id/qna1"
            android:angle="270"
            android:layout_marginLeft="110dp"
            android:layout_gravity="center"
            android:layout_width="15dp"
            android:layout_height="20dp"
            android:src="@drawable/ic_next"/>
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="5dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="370dp"
        android:layout_height="1.2dp"
        android:background="@drawable/line2"/>
    <LinearLayout
        android:id="@+id/answer1"
        android:visibility="gone"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="1.2dp"
            android:background="@drawable/line"/>
        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="wrap_content"
            android:background="#40DD3F47">
            <TextView
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="우리는 이미 승리한 게임에서 승기만 가져오면 됩니다.\n
        최선을 다한다면 그에 상응하는 대가를 받을 것입니다."/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="1.2dp"
            android:background="@drawable/line"/>
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:layout_marginLeft="20dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="그룹 초대가 오지않아요."
            android:textSize="15dp"
            android:textColor="#1C1C1C"/>
        <ImageView
            android:id="@+id/qna2"
            android:angle="270"
            android:layout_marginLeft="202dp"
            android:layout_gravity="center"
            android:layout_width="15dp"
            android:layout_height="20dp"
            android:src="@drawable/ic_next"/>
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="5dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="370dp"
        android:layout_height="1.2dp"
        android:background="@drawable/line2"/>
    <LinearLayout
        android:id="@+id/answer2"
        android:visibility="gone"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="1.2dp"
            android:background="@drawable/line"/>
        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="wrap_content"
            android:background="#40DD3F47">
            <TextView
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_width="340dp"
                android:layout_height="wrap_content"
                android:text="그룹 초대가 오지 않으시면 ~~ 이메일로 문의부탁드립니다."/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="1.2dp"
            android:background="@drawable/line"/>
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:layout_marginLeft="20dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="자료가 올라가지 않아요."
            android:textSize="15dp"
            android:textColor="#1C1C1C"/>
        <ImageView
            android:id="@+id/qna3"
            android:angle="270"
            android:layout_marginLeft="202dp"
            android:layout_gravity="center"
            android:layout_width="15dp"
            android:layout_height="20dp"
            android:src="@drawable/ic_next"/>
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="5dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="370dp"
        android:layout_height="1.2dp"
        android:background="@drawable/line2"/>
    <LinearLayout
        android:id="@+id/answer3"
        android:visibility="gone"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="1.2dp"
            android:background="@drawable/line"/>
        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="wrap_content"
            android:background="#40DD3F47">
            <TextView
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_width="340dp"
                android:layout_height="wrap_content"
                android:text="자료가 올라가지 않으시면 ~~ 이메일로 문의부탁드립니다."/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="1.2dp"
            android:background="@drawable/line"/>
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:layout_marginLeft="20dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="사진이 제대로 분류되지않아요."
            android:textSize="15dp"
            android:textColor="#1C1C1C"/>
        <ImageView
            android:id="@+id/qna4"
            android:angle="270"
            android:layout_marginLeft="161dp"
            android:layout_gravity="center"
            android:layout_width="15dp"
            android:layout_height="20dp"
            android:src="@drawable/ic_next"/>
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="5dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="370dp"
        android:layout_height="1.2dp"
        android:background="@drawable/line2"/>
    <LinearLayout
        android:id="@+id/answer4"
        android:visibility="gone"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="1.2dp"
            android:background="@drawable/line"/>
        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="wrap_content"
            android:background="#40DD3F47">
            <TextView
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_width="340dp"
                android:layout_height="wrap_content"
                android:text="사진이 제대로 분류되지 않으시면 ~~ 이메일로 문의부탁드립니다."/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="1.2dp"
            android:background="@drawable/line"/>
    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="10dp"
        android:layout_marginLeft="20dp"
        android:layout_width="70dp"
        android:layout_height="25dp"
        android:background="@drawable/logout_no">
        <TextView
            android:layout_marginLeft="10dp"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="문의하기"
            android:textColor="@color/black"/>
    </LinearLayout>

</LinearLayout>