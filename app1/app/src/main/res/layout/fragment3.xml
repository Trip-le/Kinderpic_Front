<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:background="#fffcef"
    android:orientation="vertical">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#fff2c1">
        <ImageView
            android:layout_gravity="top|right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/sun2"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="설정"
            android:textColor="@color/black"
            android:layout_margin="5dp"
            android:layout_gravity="center"
            android:textSize="30dp"/>
    </FrameLayout>
    <LinearLayout
        android:layout_marginLeft="5dp"
        android:layout_marginTop="75dp"
        android:layout_width="400dp"
        android:layout_height="118dp"
        android:background="@drawable/profile_rect_shadow">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""/>
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="80dp"
        android:layout_marginLeft="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/profile_rect">
        <LinearLayout
            android:layout_width="380dp"
            android:layout_height="100dp"
            android:orientation="horizontal">
            <ImageView
                android:layout_margin="15dp"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:src="@drawable/ic_user"/>
            <LinearLayout
                android:layout_margin="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/name_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="박고은"
                    android:layout_marginTop="10dp"
                    android:textColor="#1C1C1C"
                    android:textSize="20dp" />
                <TextView
                    android:id="@+id/email_tv"
                    android:layout_marginTop="1dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="goeun12@gmail.com"
                    android:textSize="20dp" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="350dp"
        android:layout_marginTop="120dp">
        <ImageView
            android:id="@+id/profile_next"
            android:layout_marginLeft="10dp"
            android:layout_gravity="center"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/ic_next"/>
    </FrameLayout>
    <LinearLayout
        android:layout_marginTop="195dp"
        android:layout_width="match_parent"
        android:layout_height="7dp"
        android:background="@drawable/line"/>
    <FrameLayout
        android:layout_marginTop="215dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_gravity="center_horizontal"
            android:layout_width="370dp"
            android:layout_height="1.2dp"
            android:background="@drawable/line"/>
        <TextView
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="계정 사진 업로드"
            android:textSize="20dp"
            android:textColor="#1C1C1C"/>
        <LinearLayout
            android:layout_marginTop="45dp"
            android:layout_gravity="center_horizontal"
            android:layout_width="370dp"
            android:layout_height="1.2dp"
            android:background="@drawable/line2"/>
        <ImageView
            android:id="@+id/upload_face"
            android:layout_marginLeft="150dp"
            android:layout_gravity="center"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/ic_next"/>
    </FrameLayout>
    <FrameLayout
        android:layout_marginTop="270dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_gravity="center_horizontal"
            android:layout_width="370dp"
            android:layout_height="1.2dp"
            android:background="@drawable/line"/>
        <TextView
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="이용안내"
            android:textSize="20dp"
            android:textColor="#1C1C1C"/>
        <LinearLayout
            android:layout_marginTop="45dp"
            android:layout_gravity="center_horizontal"
            android:layout_width="370dp"
            android:layout_height="1.2dp"
            android:background="@drawable/line2"/>
    </FrameLayout>
    <FrameLayout
        android:layout_marginTop="330dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="370dp"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="앱 버전"
            android:textSize="20dp"
            android:textColor="#1C1C1C"/>
        <TextView
            android:textColor="#1C1C1C"
            android:layout_marginLeft="150dp"
            android:layout_gravity="center"
            android:layout_width="40dp"
            android:layout_height="20dp"
            android:text="1.0.0"/>
    </FrameLayout>
    <FrameLayout
        android:layout_marginTop="370dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="370dp"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="공지사항"
            android:textSize="20dp"
            android:textColor="#1C1C1C"/>
        <ImageView
            android:id="@+id/notice"
            android:layout_marginLeft="150dp"
            android:layout_gravity="center"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/ic_next"/>
    </FrameLayout>
    <FrameLayout
        android:layout_marginTop="410dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="370dp"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="자주 묻는 질문"
            android:textSize="20dp"
            android:textColor="#1C1C1C"/>
        <ImageView
            android:id="@+id/qna"
            android:layout_marginLeft="150dp"
            android:layout_gravity="center"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/ic_next"/>
    </FrameLayout>
    <FrameLayout
        android:layout_marginTop="450dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="370dp"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="앱 사용 매뉴얼"
            android:textSize="20dp"
            android:textColor="#1C1C1C"/>
        <ImageView
            android:id="@+id/mannual"
            android:layout_marginLeft="150dp"
            android:layout_gravity="center"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/ic_next"/>
    </FrameLayout>
    <FrameLayout
        android:layout_marginTop="490dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="370dp"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="서비스 이용약관"
            android:textSize="20dp"
            android:textColor="#1C1C1C"/>
        <ImageView
            android:id="@+id/service"
            android:layout_marginLeft="150dp"
            android:layout_gravity="center"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/ic_next"/>
    </FrameLayout>
    <FrameLayout
        android:layout_marginTop="530dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="370dp"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="개인정보 처리방침"
            android:textSize="20dp"
            android:textColor="#1C1C1C"/>
        <ImageView
            android:id="@+id/personal_info"
            android:layout_marginLeft="150dp"
            android:layout_gravity="center"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/ic_next"/>
    </FrameLayout>
</FrameLayout>